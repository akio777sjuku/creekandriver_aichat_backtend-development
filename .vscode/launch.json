{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Quart",
      "type": "debugpy",
      "request": "launch",
      "module": "quart",
      "cwd": "${workspaceFolder}",
      "python": "${workspaceFolder}/.venv/bin/python",
      "env": {
        "QUART_APP": "main:app",
        "QUART_ENV": "development",
        "QUART_DEBUG": "0"
      },
      "args": ["run", "--no-reload", "-p 50505"],
      "console": "integratedTerminal",
      "justMyCode": false
    }
  ]
}
